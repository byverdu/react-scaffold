{
  "name": "react-scaffold",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@hot-loader/react-dom": "16.8.6",
    "@types/enzyme": "3.9.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.0.13",
    "@types/json-server": "0.14.0",
    "@types/node": "12.0.2",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "7.0.9",
    "@types/redux": "3.6.0",
    "@types/redux-actions": "2.6.1",
    "@types/redux-mock-store": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "axios": "0.19.0",
    "axios-mock-adapter": "^1.16.0",
    "classnames": "^2.2.6",
    "concurrently": "4.1.0",
    "customize-cra": "0.2.12",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.13.2",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.x",
    "eslint-config-react-app": "4.0.1",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.5.0",
    "husky": "2.3.0",
    "immutable": "4.0.0-rc.12",
    "json-server": "0.15.0",
    "lint-staged": "8.1.7",
    "merge-reducers": "1.0.0",
    "node-sass-chokidar": "1.3.4",
    "nodemon": "1.19.1",
    "prettier": "1.17.1",
    "react": "16.8.6",
    "react-app-rewired": "2.1.3",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.8.8",
    "react-redux": "7.0.3",
    "react-scripts": "3.0.1",
    "redux": "4.0.1",
    "redux-actions": "2.6.5",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "ts-node": "8.2.0",
    "typescript": "3.4.5",
    "uuid": "3.3.2"
  },
  "scripts": {
    "dev-server": "node --inspect=0.0.0.0 ./node_modules/.bin/ts-node --skip-project ./mock-server/server.ts",
    "dev-server-watch": "nodemon --exec node --inspect=0.0.0.0 ./node_modules/.bin/ts-node --skip-project  ./mock-server/server.ts --delay 500ms",
    "dev": "concurrently \"yarn start\" \"yarn dev-server\"",
    "dev-watch": "concurrently \"yarn start\" \"yarn dev-server-watch\"",
    "start": "react-app-rewired start",
    "build-css": "node-sass-chokidar src/ -o src/",
    "build": "yarn build-css && react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom --coverage --watchAll=false",
    "test-watch": "react-app-rewired test --env=jsdom",
    "lint": "./node_modules/.bin/eslint . --ext .ts,.tsx",
    "prettier": "./node_modules/.bin/prettier \"src/**/*\" --write",
    "change-remote": "./scripts/changeGitRemote.sh",
    "custom-audit": "./scripts/yarnAudit.sh",
    "clearout": "rm -rf node_modules"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/setupTests.ts",
      "!src/serviceWorker.ts",
      "!src/index.tsx",
      "!src/Models/**",
      "!src/__fixtures__/**/*"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "npm run lint",
      "pre-push": "CI=true && npm run lint && npm run test && npm run build"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {}
}
